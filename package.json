    "redux-logger": "^2.0.1",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "anybar-webpack": "^1.0.7",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.8.25",
    "concurrently": "^0.1.1",
    "eslint": "^1.6.0",
    "eslint-config-defaults": "^7.0.1",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.5.1",
    "webpack": "^1.12.2"
  }
